<div class="container1">
  <div class="row">
    <div class="col-md-2" style="background-color:#dcdcdc">
      <h4>サービス</h4>
      <p>・ランキング</p>
      <p>・殿堂入り</p>
    </div>
    <div class="col-md-7" style="background-color:#c0c0c0"></div>
    <div class="col-md-3" style="background-color:#d3d3d3"></div>
  </div>
  <div class="row1">
    <div class="wrap">
        <ul id="tab-menu">
            <li><a href="#tab1" class="active">テキスト１</a></li>
            <li><a href="#tab2" class="">テキスト２</a></li>
            <li><a href="#tab3" class="">テキスト３</a></li>
            <li><a href="#tab4" class="">テキスト４</a></li>
        </ul>
        <div id="tab-contents">
            <% @articles.each do |article| %>
            <div id="tab1" class="tab">
                <div><%= article.title %></div>
                <div><%= article.situation %></div>
             <% if current_user %>
                <div><%= button_to "編集", homes_path %></div> <!--あとでリンク先変更-->
                <div><%= button_to "削除", homes_path %></div>
             <% end %>
            </div>
            <% end %>
        </div>
    </div>
  </div>
</div>


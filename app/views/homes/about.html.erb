<div class="container about-layout">
      <div class="theTarget">
        <div class="image" style="background-image: image-url("/assets/5048383_m.jpg")"></div>
        <div class="image" style="background-image: url("/assets/gahag-0035812925.jpg")"></div>
        <div class="image" style="background-image: url("/assets/gahag-0092038731.jpg")"></div>
        <div class="image" style="background-image: url("/assets/gahag-0117363877.jpg")"></div>
        <div class="image" style="background-image: url("/assets/gahag-0119100892.jpg")"></div>
      </div>
    </div>

<div class="wrap">
        <ul id="tab-menu">
            <li><a href="#tab1" class="active">テキスト１</a></li>
            <li><a href="#tab2" class="">テキスト２</a></li>
            <li><a href="#tab3" class="">テキスト３</a></li>
            <li><a href="#tab4" class="">テキスト４</a></li>
        </ul>
        <div id="tab-contents">
            <% @articles.each do |article| %>
            <div id="tab1" class="tab">
                <div><%= article.title %></div>
                <div><%= article.situation %></div>
             <% if article.user.id == current_user.id %>
                <div><%= button_to "編集", new_article_path(article) %></div> <!--あとでリンク先変更-->
                <div><%= button_to "削除", new_article_path %></div>
             <% end %>
            </div>
            <% end %>
        </div>
    </div>